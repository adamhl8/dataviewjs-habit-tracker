{
  "name": "dataviewjs-habit-tracker",
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "bundle": "bun lint && tsdown",
    "lint": "markdown-toc -i --bullets '-' --maxdepth 3 ./README.md && ts-import-fix -w && tsc --noEmit && biome check --write && adamhl8-knip",
