    "prepublishOnly": "bun bundle"
  },
  "devDependencies": {
    "@adamhl8/configs": "^0.17.2",
    "@arethetypeswrong/core": "^0.18.2",
    "@biomejs/biome": "^2.2.4",
    "@types/bun": "^1.2.22",
    "@types/luxon": "^3.7.1",
    "knip": "^5.64.0",
    "markdown-toc": "^1.2.0",
    "obsidian-dataview": "^0.5.68",
    "publint": "^0.3.13",
    "tsdown": "^0.15.4",
    "typescript": "^5.9.2"
  }
}
